[DEFAULT]

# TMDB API Key:
tmdb_api_key = 123456

# ISO 3166-1 Region Code:
region = US

# Language Code:
# Note: using anything other than "en" will disable downloads from Apple.
lang = en

# Apple Resolution (480, 720, or 1080):
resolution = 1080

# Minimum YouTube Resolution (360, 480, 720, or 1080):
min_resolution = 480

# Maximum YouTube Resolution (360, 480, 720, or 1080):
max_resolution = 1080

# Subfolder (optional):
# You can specify a subfolder to store each downloaded trailer in.
# Example: Trailers
subfolder =

# Custom Formatting (optional):
# You can specify custom formatting to use for the name of each downloaded trailer.
# You can use the dynamic variables below and include any other characters you like.
# Variables: Title = %title%, Year = %year%
# Default: %title% (%year%)-trailer
custom_formatting =